{
  "name": "js-package-template",
  "version": "1.0.0",
  "description": "Base JavaScript NPM package template",
  "keywords": [],
  "license": "MIT",
  "author": "Gabriel Montes <gabriel@bloq.com>",
  "files": [
    "bin",
    "es5",
    "src"
  ],
  "main": "src/index.js",
  "browser": "es5/index.js",
  "bin": {
    "cli": "bin/cli.js"
  },
  "repository": "bloq/js-package-name",
  "scripts": {
    "build": "babel -d es5/ src/",
    "check": "dependency-check . && dependency-check . --unused --no-dev",
    "check:outdated": "npm-check -i eslint-config* -i husky",
    "coverage": "nyc --lines 95 --functions 95 --branches 95 --reporter=lcov --reporter=text npm test",
    "lint": "eslint --cache .",
    "precommit": "npm run lint",
    "prepublishOnly": "npm run build && npm run precommit && npm run prepush && tag-matches",
    "prepush": "npm run check && npm run coverage",
    "test": "mocha"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.6.1",
    "chai": "4.1.2",
    "check-tag-matches": "1.0.0",
    "dependency-check": "3.1.0",
    "eslint": "4.19.1",
    "eslint-config-bloq": "1.2.2",
    "eslint-config-standard": "11.0.0-beta.0",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-jsdoc": "3.5.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-standard": "3.0.1",
    "husky": "0.14.3",
    "mocha": "5.0.5",
    "npm-check": "5.6.0",
    "nyc": "11.6.0"
  },
  "preferGlobal": false,
  "private": true
}
